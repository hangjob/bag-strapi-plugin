{
    "kind": "collectionType",
    "collectionName": "bag_users",
    "info": {
        "singularName": "bag-user",
        "pluralName": "bag-users",
        "displayName": "bag-users"
    },
    "options": {
        "draftAndPublish": false,
        "comment": ""
    },
    "pluginOptions": {
        "content-manager": {
            "visible": true
        },
        "content-type-builder": {
            "visible": false
        }
    },
    "attributes": {
        "username": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "email": {
            "type": "email",
            "required": false,
            "unique": true
        },
        "phone": {
            "type": "string",
            "required": false
        },
        "nickname": {
            "type": "string",
            "required": false
        },
        "avatar": {
            "type": "string",
            "required": false
        },
        "status": {
            "type": "enumeration",
            "enum": ["active", "inactive", "banned"],
            "default": "active"
        },
        "role": {
            "type": "string",
            "default": "user"
        },
        "password": {
            "type": "string",
            "required": true
        },
        "lastLoginAt": {
            "type": "datetime",
            "required": false
        },
        "extra": {
            "type": "json"
        }
    }
}

